package LogicalProgram2;

public class DuplicateCharactar {

	public static void main(String[] args)
	{
       String a="annanaik";
       int count=0;
       for(int i=0; i<=a.length()-1;i++)
       {
    	   char Mychar = a.charAt(i);
    	   if(Mychar=='n')
    	   {
    		  // count++;
    		   System.out.println(Mychar);
    	   }
       }
      // System.out.println(count);
	}
}